<head>
  <%# Favicon related section %>
  <link rel="icon" href="<%= url('/favicon.ico') %>">
  <link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png">
  <link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png">
  <link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png">
  <link rel="manifest" href="/site.webmanifest">
  <link rel="mask-icon" href="/safari-pinned-tab.svg" color="#5bbad5">
  <meta name="msapplication-TileColor" content="#da532c">
  <meta name="theme-color" content="#ffffff">
  <%# End of Favicon related section %>
  <link href="<%= url('/style.css') %>" rel="stylesheet" type="text/css" />
</head>

<ul class='navbar'>
  <form action='/listings' method='get'>
    <input class='button' type='submit' value='Listings'>
  </form>
<% unless current_user.nil? %>
  <form action='/requests' method='get'>
    <input class='button' type='submit' value='Requests'>
  </form>
  <form action='/sessions' method='post'>
    <input id='hidden' type='hidden' name='_method' value='delete'>
    <input class='button' type='submit' value='Sign out'>
  </form>
<% else %>
  <form action='/users/new' method='GET'>
    <input class='button' type='submit' value='Sign up'>
  </form>
  <form action='/sessions/new' method='GET'>
    <input class='button' type='submit' value='Sign in'>
  </form>
<% end %>
</ul>

<%= yield %>
